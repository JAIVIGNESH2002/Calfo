<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".result">

    <TextView
        android:id="@+id/resTv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Shared successfully!"
        android:textColor="#2196F3"
        android:textSize="25sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.164"/>


    <TextView
        android:id="@+id/resName"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="40dp"
        android:background="#00FF0A"
        android:padding="5dp"
        android:text="default"
        android:textSize="25sp"
        app:layout_constraintBottom_toTopOf="@+id/resNumTv"
        app:layout_constraintEnd_toEndOf="@+id/resTv"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/resTv"
        app:layout_constraintTop_toBottomOf="@+id/resTv"
        app:layout_constraintVertical_bias="0.663" />

    <TextView
        android:id="@+id/resNumTv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="368dp"
        android:background="#00FF0A"
        android:padding="5dp"
        android:text="default"
        android:textSize="25sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/resTv"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/resTv" />


</androidx.constraintlayout.widget.ConstraintLayout>